@model Blog.ViewModels.CategoriesSiteViewModel

@{
    ViewBag.Title = "Kategoria " + Model.CategoryName;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@for (int i = 0; i < Model.Articles.Count; i++)
{
    Html.RenderPartial("_ArticleTemplate", Model.Articles[i]);
}

@{Html.RenderPartial("_Pagination");}