@model Blog.ViewModels.TagsSiteViewModel

@{
    ViewBag.Title = "Tag \"" + Model.TagName + "\"";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="sub-header">Lista artykułów zawierających tag <strong>@Model.TagName</strong> (@Model.TotalCount)</div class="sub-header">

@for (int i = 0; i < Model.Articles.Count; i++)
{
    Html.RenderPartial("_ArticleTemplate", Model.Articles[i]);
}

@{Html.RenderPartial("_Pagination");}