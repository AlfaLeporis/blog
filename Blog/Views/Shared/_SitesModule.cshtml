@model List<Blog.ViewModels.SitesModuleViewModel>

<ul>
    <li><a href="/" class="btn btn-primary">Strona główna</a></li>
    @for(int i=0; i<Model.Count; i++)
    {
        <li>@Html.ActionLink(Model[i].Title, "Site", "Sites", new{id = Model[i].Alias}, new{@class = "btn btn-primary"})</li>
    }
</ul>