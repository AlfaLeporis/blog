@model Blog.ViewModels.ArchiveViewModel

@{
    ViewBag.Title = "Archiwum od " + Model.FirstDate + " do " + Model.LastDate;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@for (int i = 0; i < Model.Articles.Count; i++)
{
    Html.RenderPartial("_ArticleTemplate", Model.Articles[i]);
}
    
@{Html.RenderPartial("_Pagination");}